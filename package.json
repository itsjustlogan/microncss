{
  "name": "microncss",
  "version": "0.2.0",
  "description": "A zero configuration self purging css utility library.",
  "main": "src/index.js",
  "bin": "src/index.js",
  "targets": {
    "main": false,
    "default": {
      "source": "src/scss/micron.scss",
      "distDir": "src/cdn"
    }
  },
  "scripts": {
    "start": "node src/index.js",
    "build:cdn": "npx parcel build src/scss/micron.scss"
  },
  "keywords": [
    "css",
    "css utility",
    "sass",
    "microncss",
    "micron",
    "zero config"
  ],
  "author": "Justin Logan",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^10.4.4",
    "eslint": "^8.11.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "gulp-purgecss": "^4.1.3",
    "parcel": "^2.4.0",
    "vitepress": "^0.22.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/itsjustlogan/microncss"
  },
  "bugs": {
    "url": "https://github.com/itsjustlogan/microncss/issues"
  },
  "dependencies": {
    "@fullhuman/postcss-purgecss": "^4.1.3",
    "cssnano": "^5.1.5",
    "postcss": "^8.4.12",
    "sass": "^1.49.9"
  }
}
